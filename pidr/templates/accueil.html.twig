    document.addEventListener("DOMContentLoaded", function () {
        console.log("JavaScript loaded!");

        document.querySelectorAll('.view-details').forEach(button => {
            button.addEventListener('click', function () {
                let freelancerId = this.getAttribute('data-id');
                console.log("Clicked button for ID:", freelancerId);

                fetch(`/matching/freelancer/details/${freelancerId}`)
                    .then(response => response.json())
                    .then(data => {
                        if (data.error) {
                            alert(data.error);
                            return;
                        }

                        console.log("Received data:", data);
                        document.getElementById('freelancerName').textContent = data.name;
                        document.getElementById('freelancerEmail').textContent = data.email;
                        document.getElementById('freelancerDOB').textContent = data.date_naissance;
                        document.getElementById('freelancerAddress').textContent = data.adresse;
                        document.getElementById('freelancerGender').textContent = data.genre;
                        document.getElementById('freelancerSpecialty').textContent = data.competences;
                        document.getElementById('freelancerBio').textContent = data.description;
                        document.getElementById('freelancerCV').textContent = data.cv;

                        // Set the freelancerId in the hidden input field
                        document.getElementById('freelancerId').value = freelancerId;

                        document.getElementById('freelancerModal').style.display = "flex";
                    })
                    .catch(error => console.error("Fetch error:", error));
            });
        });

        document.querySelector('.close').addEventListener('click', function () {
            document.getElementById('freelancerModal').style.display = "none";
        });

        // Close modal when clicking outside
        window.addEventListener('click', function(event) {
            let modal = document.getElementById('freelancerModal');
            if (event.target === modal) {
                modal.style.display = "none";
            }
        });

        document.getElementById("requestConsultation").addEventListener("click", function () {
            document.getElementById("consultationForm").style.display = "block";
        });

        document.getElementById("consultationForm").addEventListener("submit", function (event) {
            event.preventDefault();

            const freelancerId = document.getElementById("freelancerId").value;
            const date = document.getElementById("date").value;
            const adresse = document.getElementById("adresse").value;
            const mode = document.getElementById("mode").value;

            fetch("/matching/consultation/request", {  // Adjust URL if needed
                method: "POST",
                headers: { "Content-Type": "application/json" },
                body: JSON.stringify({
                    freelancerId: freelancerId,
                    date: date,
                    adresse: adresse,
                    mode: mode
                }),
            })
            .then(response => response.json())
            .then(data => {
                alert(data.message);
                if (data.success) {
                    document.getElementById("consultationForm").style.display = "none";
                }
            })
            .catch(error => console.error("Error:", error));
        });
    });
</script>
